&lt;xs:schema xmlns:xs=&quot;http://www.w3.org/2001/XMLSchema&quot;&#xD;
           xmlns:tns=&quot;http://schemas.xmlsoap.org/soap/envelope/&quot;&#xD;
           targetNamespace=&quot;http://schemas.xmlsoap.org/soap/envelope/&quot; &gt;&#xD;
&#xD;
&#xD;
  &lt;!-- Envelope, header and body --&gt;&#xD;
  &lt;xs:element name=&quot;Envelope&quot; type=&quot;tns:Envelope&quot; /&gt;&#xD;
  &lt;xs:complexType name=&quot;Envelope&quot; &gt;&#xD;
    &lt;xs:sequence&gt;&#xD;
      &lt;xs:element ref=&quot;tns:Header&quot; minOccurs=&quot;0&quot; /&gt;&#xD;
      &lt;xs:element ref=&quot;tns:Body&quot; minOccurs=&quot;1&quot; /&gt;&#xD;
&#xD;
      &lt;xs:any namespace=&quot;##other&quot; minOccurs=&quot;0&quot; maxOccurs=&quot;unbounded&quot; processContents=&quot;lax&quot; /&gt;&#xD;
    &lt;/xs:sequence&gt;&#xD;
    &lt;xs:anyAttribute namespace=&quot;##other&quot; processContents=&quot;lax&quot; /&gt;&#xD;
  &lt;/xs:complexType&gt;&#xD;
&#xD;
  &lt;xs:element name=&quot;Header&quot; type=&quot;tns:Header&quot; /&gt;&#xD;
  &lt;xs:complexType name=&quot;Header&quot; &gt;&#xD;
    &lt;xs:sequence&gt;&#xD;
      &lt;xs:any namespace=&quot;##other&quot; minOccurs=&quot;0&quot; maxOccurs=&quot;unbounded&quot; processContents=&quot;lax&quot; /&gt;&#xD;
&#xD;
    &lt;/xs:sequence&gt;&#xD;
    &lt;xs:anyAttribute namespace=&quot;##other&quot; processContents=&quot;lax&quot; /&gt;&#xD;
  &lt;/xs:complexType&gt;&#xD;
&#xD;
  &lt;xs:element name=&quot;Body&quot; type=&quot;tns:Body&quot; /&gt;&#xD;
  &lt;xs:complexType name=&quot;Body&quot; &gt;&#xD;
    &lt;xs:sequence&gt;&#xD;
      &lt;xs:any namespace=&quot;##any&quot; minOccurs=&quot;0&quot; maxOccurs=&quot;unbounded&quot; processContents=&quot;lax&quot; /&gt;&#xD;
    &lt;/xs:sequence&gt;&#xD;
    &lt;xs:anyAttribute namespace=&quot;##any&quot; processContents=&quot;lax&quot; &gt;&#xD;
&#xD;
	  &lt;xs:annotation&gt;&#xD;
	    &lt;xs:documentation&gt;&#xD;
		  Prose in the spec does not specify that attributes are allowed on the Body element&#xD;
		&lt;/xs:documentation&gt;&#xD;
	  &lt;/xs:annotation&gt;&#xD;
	&lt;/xs:anyAttribute&gt;&#xD;
  &lt;/xs:complexType&gt;&#xD;
&#xD;
&#xD;
  &lt;!-- Global Attributes.  The following attributes are intended to be usable via qualified attribute names on any complex type referencing them.  --&gt;&#xD;
  &lt;xs:attribute name=&quot;mustUnderstand&quot; &gt;&#xD;
     &lt;xs:simpleType&gt;&#xD;
&#xD;
     &lt;xs:restriction base='xs:boolean'&gt;&#xD;
	   &lt;xs:pattern value='0|1' /&gt;&#xD;
	 &lt;/xs:restriction&gt;&#xD;
   &lt;/xs:simpleType&gt;&#xD;
  &lt;/xs:attribute&gt;&#xD;
  &lt;xs:attribute name=&quot;actor&quot; type=&quot;xs:anyURI&quot; /&gt;&#xD;
&#xD;
  &lt;xs:simpleType name=&quot;encodingStyle&quot; &gt;&#xD;
    &lt;xs:annotation&gt;&#xD;
&#xD;
	  &lt;xs:documentation&gt;&#xD;
	    'encodingStyle' indicates any canonicalization conventions followed in the contents of the containing element.  For example, the value 'http://schemas.xmlsoap.org/soap/encoding/' indicates the pattern described in SOAP specification&#xD;
	  &lt;/xs:documentation&gt;&#xD;
	&lt;/xs:annotation&gt;&#xD;
    &lt;xs:list itemType=&quot;xs:anyURI&quot; /&gt;&#xD;
  &lt;/xs:simpleType&gt;&#xD;
&#xD;
  &lt;xs:attribute name=&quot;encodingStyle&quot; type=&quot;tns:encodingStyle&quot; /&gt;&#xD;
  &lt;xs:attributeGroup name=&quot;encodingStyle&quot; &gt;&#xD;
    &lt;xs:attribute ref=&quot;tns:encodingStyle&quot; /&gt;&#xD;
&#xD;
  &lt;/xs:attributeGroup&gt;&#xD;
&#xD;
  &lt;xs:element name=&quot;Fault&quot; type=&quot;tns:Fault&quot; /&gt;&#xD;
  &lt;xs:complexType name=&quot;Fault&quot; final=&quot;extension&quot; &gt;&#xD;
    &lt;xs:annotation&gt;&#xD;
	  &lt;xs:documentation&gt;&#xD;
	    Fault reporting structure&#xD;
	  &lt;/xs:documentation&gt;&#xD;
	&lt;/xs:annotation&gt;&#xD;
    &lt;xs:sequence&gt;&#xD;
&#xD;
      &lt;xs:element name=&quot;faultcode&quot; type=&quot;xs:QName&quot; /&gt;&#xD;
      &lt;xs:element name=&quot;faultstring&quot; type=&quot;xs:string&quot; /&gt;&#xD;
      &lt;xs:element name=&quot;faultactor&quot; type=&quot;xs:anyURI&quot; minOccurs=&quot;0&quot; /&gt;&#xD;
      &lt;xs:element name=&quot;detail&quot; type=&quot;tns:detail&quot; minOccurs=&quot;0&quot; /&gt;&#xD;
    &lt;/xs:sequence&gt;&#xD;
  &lt;/xs:complexType&gt;&#xD;
&#xD;
  &lt;xs:complexType name=&quot;detail&quot;&gt;&#xD;
    &lt;xs:sequence&gt;&#xD;
      &lt;xs:any namespace=&quot;##any&quot; minOccurs=&quot;0&quot; maxOccurs=&quot;unbounded&quot; processContents=&quot;lax&quot; /&gt;&#xD;
&#xD;
    &lt;/xs:sequence&gt;&#xD;
    &lt;xs:anyAttribute namespace=&quot;##any&quot; processContents=&quot;lax&quot; /&gt;&#xD;
  &lt;/xs:complexType&gt;&#xD;
&#xD;
&lt;/xs:schema&gt;</param>
            <param name="XSDhttp://schemas.xmlsoap.org/soap/encoding/">&lt;xs:schema targetNamespace=&quot;http://schemas.xmlsoap.org/soap/encoding/&quot; xmlns:tns=&quot;http://schemas.xmlsoap.org/soap/encoding/&quot; xmlns:xs=&quot;http://www.w3.org/2001/XMLSchema&quot;&gt;

 &lt;xs:attribute name=&quot;root&quot;&gt;
