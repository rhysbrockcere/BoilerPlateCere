  &lt;/xs:attributeGroup&gt;

  &lt;xs:attribute name=&quot;position&quot; type=&quot;tns:arrayCoordinate&quot;/&gt;

  &lt;xs:attributeGroup name=&quot;arrayMemberAttributes&quot;&gt;

    &lt;xs:attribute ref=&quot;tns:position&quot;/&gt;
  &lt;/xs:attributeGroup&gt;

  &lt;xs:group name=&quot;Array&quot;&gt;
    &lt;xs:sequence&gt;
      &lt;xs:any maxOccurs=&quot;unbounded&quot; minOccurs=&quot;0&quot; namespace=&quot;##any&quot; processContents=&quot;lax&quot;/&gt;
